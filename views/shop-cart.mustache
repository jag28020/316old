	<section id="page-body" class="woocommerce">
    	
		<div class="wrapper">
     	     		
     	  	<form action="#" method="post">

				<table class="shop_table cart" cellspacing="0">
					<thead>
						<tr>
							<th class="product-thumbnail">&nbsp;</th>
							<th class="product-name"><strong>Product</strong></th>
							<th class="product-price"><strong>Price</strong></th>
							<th class="product-quantity"><strong>Quantity</strong></th>
							<th class="product-subtotal"><strong>Total</strong></th>
						</tr>
					</thead>
					<tbody>
						<tr class="cart_item">
							<td class="product-thumbnail">
								<a href="product-simple.html"><img width="100" height="100" src="files/uploads/100x100.jpg" alt="SEO" /></a>
							</td>
							<td class="product-name">
								<h5><a href="product-simple.html">The Wallet</a></h5>
							</td>
							<td class="product-price">
								<span class="amount"><span class="woocommerce-Price-currencySymbol">&#36;</span>49.00</span>
							</td>
							<td class="product-quantity">
								<div class="quantity">
									<span class="qty-button plus"></span>
									<input type="number" step="1" min="0" name="qty" value="1" class="input-text qty text" />
									<span class="qty-button minus"></span>
								</div>
								<a href="#" class="remove" title="Remove this item">&times;</a>
							</td>
							<td class="product-subtotal">
								<span class="amount"><span class="woocommerce-Price-currencySymbol">&#36;</span>49.00</span>
							</td>
						</tr>
						
						<tr class="cart_item">
							<td class="product-thumbnail">
								<a href="product-simple.html"><img width="100" height="100" src="files/uploads/100x100.jpg" alt="SEO" /></a>
							</td>
							<td class="product-name">
								<h5><a href="product-simple.html">Tea Cup</a></h5>
							</td>
							<td class="product-price">
								<span class="amount"><span class="woocommerce-Price-currencySymbol">&#36;</span>39.00</span>
							</td>
							<td class="product-quantity">
								<div class="quantity">
									<span class="qty-button plus"></span>
									<input type="number" step="1" min="0" name="qty" value="1" class="input-text qty text" />
									<span class="qty-button minus"></span>
								</div>
								<a href="#" class="remove" title="Remove this item">&times;</a>
							</td>
							<td class="product-subtotal">
								<span class="amount"><span class="woocommerce-Price-currencySymbol">&#36;</span>39.00</span>
							</td>
						</tr>
					</tbody>
				</table>


				<div class="cart-actions">
					<input type="submit" class="button" name="update_cart" value="Update Cart" />
				</div>
				
				<div class="coupon">
					<label for="coupon_code">Coupon:</label>
					<input type="text" name="coupon_code" id="coupon_code" value="" placeholder="Coupon code" />
					<input type="submit" class="button" name="apply_coupon" value="Apply Coupon" />
				</div>
				
				<div class="clear"></div>

				<div class="cart-collaterals">
					<div class="cart_totals">
						<table cellspacing="0" class="shop_table">
							<tr class="cart-subtotal">
								<th>Subtotal</th>
								<td><span class="amount"><span class="woocommerce-Price-currencySymbol">&#36;</span>88.00</span></td>
							</tr>
							<tr class="order-total">
								<th>Total</th>
								<td><strong><span class="amount"><span class="woocommerce-Price-currencySymbol">&#36;</span>88.00</span></strong></td>
							</tr>
						</table>
						
